import java.util.Scanner;
class HelloWorld {
    public static void main(String[] args) 
    {
        int count=0;
        System.out.println("enter a number");
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        while(a>0)
        {
            int rem=a%10;
            count++;
            a=a/10;
        }
        System.out.println("count of number is = "+count);
    }
}
