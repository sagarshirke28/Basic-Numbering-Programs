import java.util.Scanner;
class HelloWorld {
    public static void main(String[] args) 
    {
        System.out.println("enter a number");
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        while(a>0)
        {
            System.out.print(a%10);
            a=a/10;
        }
    }
}
